<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="5dp"/>
            <stroke android:width="1dp" android:color="@color/cancelButtonPressed"/>
            <solid android:color="@color/cancelButtonPressed"/>
        </shape>

    </item>


    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/cancelButtonNotPressed"/>
            <corners android:radius="5dp"/>
            <stroke android:width="1dp" android:color="@color/cancelButtonNotPressed"/>
        </shape>
    </item>
</selector>